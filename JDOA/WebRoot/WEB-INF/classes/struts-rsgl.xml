<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="utf-8"></constant>
	<!-- 人事管理模块 -->
	<package name="rsgl" extends="struts-default" namespace="/">
		<action name="RedeployList" class="com.jdoa.basic.rsgl.RedeployAction" method="RedeployList">
			<param name="aliasname">人员调动列表</param>
			<param name="authoritytype">-1</param>
		</action>
		
		<action name="addOrEditRedeploy" class="com.jdoa.basic.rsgl.RedeployAction" method="addOrEditRedeploy">
			<param name="aliasname">人员调动添加或修改</param>
			<param name="authoritytype">-1</param>
		</action>
		
		<action name="deleteRedeploy" class="com.jdoa.basic.rsgl.RedeployAction" method="deleteRedeploy">
			<param name="aliasname">人员调动删除</param>
			<param name="authoritytype">-1</param>
		</action>
		
	</package>

</struts>